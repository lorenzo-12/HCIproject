<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestGoogle"
    android:background="@drawable/background_tablet">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/google_female_button"
        android:layout_width="236dp"
        android:layout_height="127dp"
        android:layout_marginEnd="24dp"
        android:background="@color/grey"
        android:text="female"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/google_male_button"
        app:layout_constraintTop_toTopOf="@+id/google_male_button"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/google_male_button"
        android:layout_width="236dp"
        android:layout_height="127dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="164dp"
        android:background="@color/grey"
        android:text="male"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.011" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/google_signup_button"
        android:layout_width="266dp"
        android:layout_height="105dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/custom_button"
        android:text="sign in"
        android:textColor="@color/grey"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_kal_goal"
        app:layout_constraintVertical_bias="0.095" />

    <EditText
        android:id="@+id/google_signup_weight"
        android:layout_width="279dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/black"
        android:drawableEnd="@drawable/kg_image"
        android:ems="6"
        android:hint="weight"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_male_button"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/google_signup_height"
        android:layout_width="279dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/black"
        android:drawableEnd="@drawable/height_image"
        android:ems="6"
        android:hint="height"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toEndOf="@+id/google_signup_weight"
        app:layout_constraintTop_toBottomOf="@+id/google_female_button"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/google_signup_carb_goal"
        android:layout_width="575dp"
        android:layout_height="90dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/black"
        android:drawableEnd="@drawable/grams_image"
        android:ems="10"
        android:hint="Daily carbs goal"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_height"
        app:layout_constraintVertical_bias="0.1" />

    <EditText
        android:id="@+id/google_signup_prot_goal"
        android:layout_width="575dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/black"
        android:drawableEnd="@drawable/grams_image"
        android:ems="10"
        android:hint="Daily proteins goal"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_carb_goal"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/google_signup_fat_goal"
        android:layout_width="575dp"
        android:layout_height="90dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/black"
        android:drawableEnd="@drawable/grams_image"
        android:ems="10"
        android:hint="Daily fats goal"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_prot_goal"
        app:layout_constraintVertical_bias="0.018" />

    <EditText
        android:id="@+id/google_signup_kal_goal"
        android:layout_width="575dp"
        android:layout_height="90dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/black"
        android:ems="10"
        android:hint="Daily calories goal"
        android:inputType="number"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_fat_goal"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/google_error_signupinfopage_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:text="TextView"
        android:textColor="@color/red"
        android:textSize="34sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google_signup_button" />

</androidx.constraintlayout.widget.ConstraintLayout>